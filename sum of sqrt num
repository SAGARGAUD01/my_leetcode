class Solution {
public:

    bool judgeSquareSum(int c) {
         for (long a = 0; a * a <= c; ++a) { // a*a <= c
        double b = sqrt(c - a * a);  //storing sqrt of c - a*a in b var
        if (b == static_cast<int>(b)) {
            return true;
        }
    }
    return false;
    }
    
};s
